<ol class="breadcrumb">
    <%if(exists){%>
        <label class="error text-danger">"Car with same car name and machine id already exists"</label>
        <%}%>
</ol>
<h1 class='display-4'>Add Cars</h1>
<form method="POST" action="/jobs/create">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Car Name</th>
                <th>Car Part Id</th>
                <th>Required Qty</th>
            </tr>
        </thead>
        <tbody>
            <td><input type="text" pattern="[A-Z][A-Za-z0-9]{1,9}" name="jobName05" title="Start with capital letter, Continue with numbers or letters of max length 9" required></input></td>
            <td><select name="partId05" >
                <% partsList05.forEach(function(part){%>  
                    <option value="<%=part%>"><%=part%></option>
                <% })%>
              </select></td>
            <td><input type="number" min="1" max="10000" name="qty05" required></input></td>
        </tbody>
        </table>
        <input type="submit" value="submit" class="btn btn-primary">
</form>